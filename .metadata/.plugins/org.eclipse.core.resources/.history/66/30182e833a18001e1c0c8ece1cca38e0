package Java_Program;

import javax.naming.spi.DirStateFactory.Result;

public class fact {
	
	static int factorial(int a )
	{
		int result = 0;
		for(int i=0;i>0;i--)
		{
		  result*=i;	
		}
		return result;
		
	}
	public static void main(String[] args) {
		factorial(5);
		System.out.println("Factorial of 5 is:"+factorial(5));
		
	}

}
